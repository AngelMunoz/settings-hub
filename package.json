{
  "name": "tun-settings-hub",
  "version": "0.0.2",
  "description": "Tun Settings Hub",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "module": "dist/esm/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --prerender",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "@stencil/core": "0.13.0-5",
    "@stencil/router": "0.2.5"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "jest": "^23.4.2",
    "workbox-build": "3.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AngelMunoz/settings-hub.git"
  },
  "author": "Angel Munoz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AngelMunoz/settings-hub"
  },
  "homepage": "https://github.com/AngelMunoz/settings-hub",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
